    FACT_SETTINGS_BASE:=    0x00004000
    CODE_BASE     :=        0x00006000
    USER_DATA_BASE:=        0x00700000
    USER_DATA_SIZE:=        0x00100000
    USER_DATA_BLOCK_SIZE:=  0x00000200

